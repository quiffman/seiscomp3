<?xml version="1.0" encoding="UTF-8"?>
<seiscomp>
	<binding module="seedlink" name="nrts" category="sources">
		<description>NRTS (TCP/IP)</description>
		<configuration>
			<parameter name="address" type="string" default="idahub.ucsd.edu">
				<description>Hostname or IP of the NRTS server.</description>
			</parameter>
			<parameter name="port" type="int" default="39136">
				<description>Port of the NRTS server.</description>
			</parameter>
			<parameter name="streams" type="string" default="bhz.00,bhn.00,bhe.00">
				<description>List of streams to request.</description>
			</parameter>
			<parameter name="proc" type="string" default="nrts">
				<description>
				Name of the proc object (defined in streams.xml); used for processing
				raw streams (streams submitted by a plugin as raw samples).
				</description>
			</parameter>
		</configuration>
	</binding>
</seiscomp>
